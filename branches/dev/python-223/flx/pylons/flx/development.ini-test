qualname = beaker.container
propagate = 1

[logger_mongodb_beaker]
level = INFO
handlers =
qualname = mongodb_beaker
propagate = 1

[handler_console]
class = StreamHandler
args = (sys.stderr,)
level = NOTSET
formatter = generic

[handler_flxlog]
class = flx.lib.safeloghandler.TimedRotatingFileHandlerSafe
args = ('/opt/2.0/log/flx.log', 'h', 12, 14, 'utf-8')
level = NOTSET
formatter = generic

[handler_slowapi]
class = flx.lib.safeloghandler.TimedRotatingFileHandlerSafe
args = ('/opt/2.0/log/slowapi.flx.log', 'h', 12, 14, 'utf-8')
level = NOTSET
formatter = generic 

[handler_save_errors_flx]
class = flx.lib.safeloghandler.TimedRotatingFileHandlerSafe
args = ('/opt/2.0/log/save_errors_flx.log', 'h', 12, 14, 'utf-8')
level = NOTSET
formatter = generic

[handler_wsgierrors]
class = pylons.log.WSGIErrorsHandler
args = ()
level = ERROR
formatter = generic

[formatter_generic]
format = %(asctime)s,%(msecs)03d %(levelname)-5.5s %(threadName)s [%(name)s] %(message)s
datefmt = %Y-%m-%d %H:%M:%S
