    WSGIScriptAlias / /opt/2.0/flxweb/flxweb.wsgi
    <Directory /opt/2.0/flxweb/>
        Require all granted
    </Directory>

    RewriteEngine On
    RewriteCond %{REQUEST_URI} !^/media/build-.*
