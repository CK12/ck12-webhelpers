#
# Peer Help configuration that allows all requests
#

WSGIScriptAlias /peerhelp/api /opt/2.0/peerhelp/core/pyramid/peer_help.wsgi

<Directory /opt/2.0/peerhelp/core/pyramid/>
    #Order allow,deny
    #Allow from all
	Require all granted
</Directory>
<Location /peerhelp/api>
    WSGIProcessGroup peerhelp_wsgi
</Location>

