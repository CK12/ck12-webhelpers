        location /taxonomy {
            proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
            proxy_set_header Host $http_host;
            proxy_redirect off;


            rewrite /taxonomy/(media/.*)$ /$1 break;
            proxy_pass http://paster_sts;

            proxy_cache   CACHE;
            proxy_cache_valid  200 302  60m;
            proxy_cache_valid  404      1m;
        }

