{
    "extends": "eslint-config-defaults",
    "rules": {
        "indent": [
            1,
            4,
            {"SwitchCase": 1}
        ],
        "quotes": [
            1,
            "single"
        ],
        "linebreak-style": [
            0,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "camelcase": [0],
        "no-underscore-dangle": [0],
        "new-cap": [0],
        "key-spacing": [0],
        "no-console": [0],
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error",
    },
    "env": {
        "browser": true,
        "amd": true
    },
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins":[
        "react"
    ]
}
