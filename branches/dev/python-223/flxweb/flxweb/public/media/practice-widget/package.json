{
  "name": "ck12-practice-widget",
  "version": "0.0.1",
  "description": "A widget to render assessment frame",
  "main": "src/main.js",
  "scripts": {
    "test": "nyc mocha --opts .mocharc tests/*",
    "test:junit": "nyc mocha --opts .mocharcjunit tests/*",
    "build:dev": "webpack --config webpack.config.js --includeDeps",
    "build": "NODE_ENV=production webpack --config webpack.config.js --includeDeps",
    "lint": "eslint src.*.js",
