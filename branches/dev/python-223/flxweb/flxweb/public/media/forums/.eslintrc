{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "camelcase": [0],
        "no-underscore-dangle": [0],
        "new-cap": [0],
        "key-spacing": [0],
        "no-console": [0]
    },
    "env": {
        "browser": true,
        "amd": true,
        "node": true
    },
    "extends": "eslint:recommended"
}
